URxvt.termName: rxvt-256color

XTerm.vt100.saveLines: 4096

*faceName: Ubuntu Mono
*faceSize: 12
*renderFont: true

URxvt*depth: 32

! URxvt*foreground: grey
!URxvt*background: rgba:0000/0000/0200/e500

!// URxvt*background: rgba:


*selectToClipboard: true

URxvt*font: xft:Monaco:style=Regular:size=12:antialias=true
! // URxvt.font: xft:Menlo:size=12:antialias=true
URxvt*boldFont: xft:Monaco:bold:size=12:antialias=true

URxvt*background: black
URxvt*foreground: grey

URxvt.scrollBar: false

URxvt.keysym.Control-Left:\033[1;5D

URxvt.keysym.Control-Right:\033[1;5C

URxvt.perl-ext-common: ...,font-size

URxvt.keysym.Meta-Up:     perl:font-size:increase
URxvt.keysym.Meta-Down:   perl:font-size:decrease

xterm*background: black
xterm*foreground: lightgray

! // URxvt.keysym.Meta:

Emacs.FontBackend: xft,x
Emacs.font: Monaco:size=12:weight=light
XTerm*metaSendsEscape: true


*.font:deja vu sans mono:size=14:antialias=true:autohint=true
*.color0: #000

*.alpha: 0.8


*.background: #000